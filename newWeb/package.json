    "@mui/x-date-pickers-pro": "^7.18.0",
    "axios": "^1.7.7",
    "date-fns": "2.13.0",
    "dayjs": "^1.11.13",
    "echarts": "^5.5.0",
    "echarts-for-react": "^3.0.2",
    "react": "^18.2.0",
